{
	"presets": [
		["@babel/preset-env", {"modules": false}],
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-proposal-object-rest-spread",
		["css-modules-transform", {
			"extensions": [".css"],
			"generateScopedName": "[hash:base64:5]", 
			"mode": "local",
			"extractCss": "./dist/styles.css",
			"prepend": [
				"postcss-mixins",
			]
		}]
	]
}
